@using System.ComponentModel.DataAnnotations
@model string

@if (@ViewData.ModelMetadata.DataTypeName == DataType.Html.ToString())
{
    @Html.Raw(Model)
}
else if (@ViewData.ModelMetadata.DataTypeName == DataType.MultilineText.ToString())
{
    <pre>
        @Model
    </pre>
}
else
{
    @Model
}
